@model IEnumerable<Film>;
@{
    ViewData["Title"]="Kategoria " + Model.First().Category.Name;
}
<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Director</th>
            <th>Description</th>
            <th>Price</th>

        </tr>

    </thead>
    <tbody>
        @foreach (var film in Model)
        {
            <tr>
                <td>@film.Title</td>
                <td>@film.Director</td>
                <td>@film.Desc</td>
                <td>@film.Price</td>


            </tr>
        }
    </tbody>
</table>